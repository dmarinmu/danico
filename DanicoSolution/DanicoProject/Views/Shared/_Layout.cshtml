   <script  type="text/javascript">

       preloadImages([
			'images/nav-a-left.png',
			'images/nav-a-right.png',
			'images/nav-a-tail.png'
		])
       $(document).ready(function () {
           if ($(document).width() > 768) { $("#portfolio a[rel^='prettyPhoto']").prettyPhoto({}); }
           /*Filter*/
           $('.filter_list a').click(function () {
               $('.filter_list li').removeClass('act');
               $(this).parent('li').addClass('act');
               filter_class = $(this).attr('rel');
               dm_filter('#portfolio', '#portfolio div.columns', filter_class, '1750');

           });

           function dm_filter($obj, $elem_type, $filter, $timer) {
               $half = $timer / 2;
               if ($filter == "*") {
                   $($obj).find($elem_type).show($timer);

               }
               else {
                   $($obj).find($elem_type).not($filter).hide($half, function () {
                       $($obj).find($filter).show($timer, function () {
                       });
                   });
               }
           }

       });
   </script>

@{
    ViewBag.Title = "Delete";
    Layout = null;
}
<!doctype html>
<html lang="en">
<head>
    
    <link href="../../Content/css/Autocomplete/jquery-ui-1.8.24.custom.css" rel="stylesheet"
        type="text/css" />
    <script src="../../Content/jQuery/Autocomplete/jquery-1.8.2.min.js" type="text/javascript"></script>
    <script src="../../Content/jQuery/Autocomplete/jquery-ui-1.8.24.custom.min.js" type="text/javascript"></script>

    <script type="text/javascript">
      
        $(function () {
            var availableTags = [
			"ActionScript",
			"AppleScript",
			"Asp",
			"BASIC",
			"C",
			"C++",
			"Clojure",
			"COBOL",
			"ColdFusion",
			"Erlang",
			"Fortran",
			"Groovy",
			"Haskell",
			"Java",
			"JavaScript",
			"Lisp",
			"Perl",
			"PHP",
			"Python",
			"Ruby",
			"Scala",
			"Scheme"
		];
            $("#tags").autocomplete({
                source: availableTags
            });

            $("#SearchString").autocomplete({
                source: "/Quote/AutocompleteSuggestions",
                minLength: 1,
                select: function (event, ui) {
                    if (ui.item) {
                        $("#SearchString").val(ui.item.value);
                        $("form").submit();
                    }
                }
            });
        });

    </script>

</head>
<body>
</body>
</html>

@using (Html.BeginForm())
{
    <p>
    Find by name: @Html.TextBox("SearchString")  
    <input type="submit" value="Search" /></p>
}

<div class="ui-widget">
	<label for="tags">Tags: </label>
	<input id="tags">
</div>